    "core-js": "^2.6.9",
    "moment": "^2.24.0",
    "store": "^2.0.12",
    "vue": "^2.6.10",
    "vue-chartjs": "^3.4.2",
    "vue-i18n": "^8.11.2",
    "vue-router": "^3.0.6",
    "vue-tweet-embed": "^2.4.0",
    "vuex": "^3.1.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.8.0",
    "@vue/cli-plugin-eslint": "^3.8.0",
    "@vue/cli-service": "^3.8.0",
    "@vue/eslint-config-airbnb": "^4.0.0",
